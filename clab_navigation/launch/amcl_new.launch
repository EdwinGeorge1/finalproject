<?xml version="1.0"?>
<launch>
  <arg name="map"     default="$(find clab_navigation)/maps"/>
  <node name="map_node" pkg="map_server" type="map_server" args="$(arg map)/new_corr.yaml" respawn="false">
  </node>

  <node pkg="amcl" type="amcl" name="amcl" output="screen">

      <rosparam file="$(find clab_navigation)/config/amcl.yaml" command="load"/>

      <param name="use_map_topic" value="true"/>
  </node>

</launch>

