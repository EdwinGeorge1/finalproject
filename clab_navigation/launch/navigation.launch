<?xml version="1.0"?>

<launch>

    <arg name="model" default="waffle" doc="model type [burger, waffle, waffle_pi]"/>
    <arg name="open_rviz" default="true"/>
    <arg name="move_forward_only" default="false"/>

    <!-- <include file="$(find rplidar_ros)/launch/view_rplidar.launch"/> -->
    <include file="$(find rplidar_ros)/launch/rplidar.launch"/>
    <arg name="map_file" default="$(find clab_navigation)/maps/new_corr.yaml"/>
    <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)"/>

    <!-- <include file="$(find laser_filters)/examples/sector_filter_example.launch" />-->

    <include file="$(find clab_navigation)/launch/move_basee.launch"/>

    <include file="$(find clab_navigation)/launch/amcl.launch" />


</launch>

